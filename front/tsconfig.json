{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "module": "es2020",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "importHelpers": true,
        "target": "es2020",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2020",
            "dom"
        ]
    },
    "skipLibCheck": true
}